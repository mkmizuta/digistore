<h1>Digital Directory</h1>
<div class='row'>
  <div class='col-lg-2'>
    <h4>All Things Digital</h4>
    <ul class='list-unstyled'>
      {{# each }}
        <li>
          {{# link-to "product" this }}
          <img {{ bind-attr src=avatar }} class='thumbnail col-lg-12' /> 
            {{ name }}
          {{/ link-to }}
          ${{ format-price price }}
        </li>

      {{/ each }}
    </ul>
  </div>
  <div class='col-lg-10'>
    {{ outlet }}
  </div>
</div>
